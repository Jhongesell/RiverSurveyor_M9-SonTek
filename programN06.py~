# Dropping Rows and Columns in pandas Dataframe
import pandas as pd

data = {'name': ['Jason', 'Molly', 'Tina', 'Jake', 'Amy'],
        'year': [2012, 2012, 2013, 2014],
        'repots': [4, 24, 31, 2, 3]}
df = pd.DataFrame(data, index = ['Choice', 'Pima', 'Santa Cruz',
                                 'Maricopa', 'Yuma'])
print(df)
